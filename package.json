{
  "name": "threejs-spherical-controls",
  "version": "0.4.4",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-spherical-controls.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "keywords": [
    "threejs"
  ],
  "dependencies": {
    "tween.js-ticker": "github:MasatoMakino/tween.js-ticker"
  },
  "peerDependencies": {
    "@tweenjs/tween.js": ">=18.6.4 <19.0.0",
    "three": ">=0.126.0 <1.0.0"
  },
  "devDependencies": {
    "@tweenjs/tween.js": "18.6.4",
    "@types/jest": "^27.4.1",
    "@types/three": "^0.139.0",
    "gulptask-demo-page": "github:MasatoMakino/gulptask-demo-page",
    "gulptask-dev-server": "github:MasatoMakino/gulptask-dev-server",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lil-gui": "^0.16.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "three": "^0.139.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typescript": "4.6.3"
  },
  "scripts": {
    "prepare": "husky install",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run demo & npm run buildTS",
    "demo": "npx gulptask-demo-page --body '<canvas id=\"webgl-canvas\" width=\"1920\" height=\"1080\"></canvas>'",
    "buildTS": "npx tsc --project tsconfig.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx gulptask-dev-server -b ./docs/demo",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.json & tsc -W --incremental --project tsconfig.esm.json"
  }
}
